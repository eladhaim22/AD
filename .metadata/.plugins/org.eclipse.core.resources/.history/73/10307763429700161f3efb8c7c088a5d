package dominio;

import javax.persistence.CascadeType;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.OneToOne;
import javax.persistence.Table;

@Entity
@Table (name="PAQUETES")
public class Paquete{
	
	@Id
	@Column(nullable=false, columnDefinition="varchar(30)", name="codPaquete")
	private String codigo;
	
	@Column(name="peso")
	private float peso;
	
	@Column(name="alto")
	private float alto;
	
	@Column(name="ancho")
	private float ancho;
	
	@Column(name="profundidad")
	private float profundidad;
	
	@Column(name="fragilidad")
	private String fragilidad;
	
	@Column(name="tratamiento")
	private String tratamiento;
	
	@ManyToOne
	@JoinColumn(name="codDestino") 
	private Destino destino;
	

	@ManyToOne
	@JoinColumn(name="codManifiesto")
	private ManifiestoPaquete manifiesto;
	
	
	
}
