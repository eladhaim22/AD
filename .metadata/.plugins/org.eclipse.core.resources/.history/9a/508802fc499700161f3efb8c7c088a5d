package dto;

import java.io.Serializable;
import java.rmi.RemoteException;

public class TestDto implements ITestDto{
	
	private static final long serialVersionUID = 1L;
	private int id;
	private String test;

	public TestDto(int id, String test) throws RemoteException{
		super();
		this.id = id;
		this.test = test;
	}

	public static long getSerialversionuid() throws RemoteException{
		return serialVersionUID;
	}

	public int getId() throws RemoteException{
		return id;
	}

	public void setId(int id) throws RemoteException{
		this.id = id;
	}

	public String getTest() throws RemoteException{
		return test;
	}

	public void setTest(String test) throws RemoteException{
		this.test = test;
	}
	
}
