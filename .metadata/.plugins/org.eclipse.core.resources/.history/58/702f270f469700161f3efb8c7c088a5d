package dominio;

import java.util.List;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.OneToMany;
import javax.persistence.Table;

@Entity
@Table(name="PLANES_DE_MANTENIMIENTO")
public class PlanDeMantenimiento {
	@Id
	@Column(nullable=false, columnDefinition="varchar(30)", name="codPlanDeMantenimiento")
	private String codigo;
	@OneToMany @JoinColumn (name="codOperacionDeMantenimiento")
	private List<OperacionDeMantenimiento> operacionesDeMantenimiento;
}
