package dominio;
import java.util.List;
import java.util.Vector;

import javax.persistence.*;

@Entity
@Inheritance(strategy=InheritanceType.SINGLE_TABLE)
@Table(name="MANIFIESTOS")
@DiscriminatorColumn(name="tipo_manifiesto",discriminatorType=DiscriminatorType.STRING)
public class Manifiesto{
	@Id
	@Column(nullable=false, columnDefinition="varchar(30)", name="codManifiesto")
	private String codigo;
	@OneToMany @JoinColumn (name="codEnvioTransportado") 
	private List<EnvioTransportado> enviosTransportados;
	
}
